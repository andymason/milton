{
  "name": "figma2html",
  "version": "0.7.0",
  "description": "Export frames from Figma to HTML",
  "main": "index.tsx",
  "scripts": {
    "dev": "watchexec --exts css,scss,html,tsx,ts,json --clear -w ./src node ./build.js",
    "build": "NODE_ENV=production node build.js",
    "lint": "deno lint src",
    "typecheck": "tsc",
    "test": "uvu -r ts-node/register ./src"
  },
  "author": "Andrew Mason",
  "license": "ISC",
  "devDependencies": {
    "@figma/plugin-typings": "^1.37.0",
    "@pdf-lib/upng": "^1.0.1",
    "@types/file-saver": "^2.0.3",
    "@types/pica": "^5.1.3",
    "@types/resize-observer-browser": "^0.1.6",
    "clipboard-copy": "^4.0.1",
    "esbuild": "^0.13.9",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "indexeddbshim": "^8.0.0",
    "just-throttle": "^3.0.0",
    "node-html-parser": "^5.0.0",
    "pica": "7.1.0",
    "preact": "^10.5.15",
    "preact-render-to-string": "^5.1.19",
    "react-colorful": "^5.5.0",
    "svgo": "^2.7.0",
    "typescript": "^4.4.4",
    "uvu": "^0.5.2"
  },
  "sideEffects": false,
  "type": "module",
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": false
  }
}
