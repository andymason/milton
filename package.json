{
  "name": "figma2html",
  "version": "0.7.0",
  "description": "Export frames from Figma to HTML",
  "main": "index.tsx",
  "scripts": {
    "dev": "watchexec --exts css,scss,html,tsx,ts,json --clear -w ./src node ./build.js",
    "build": "NODE_ENV=production node build.js",
    "lint": "deno lint src",
    "test": "uvu -r ts-node/register ./src"
  },
  "author": "Andrew Mason",
  "license": "ISC",
  "devDependencies": {
    "@figma/plugin-typings": "^1.24.0",
    "@types/file-saver": "^2.0.2",
    "@types/pica": "^5.1.2",
    "@types/resize-observer-browser": "^0.1.5",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "clipboard-copy": "^4.0.1",
    "esbuild": "^0.12.8",
    "file-saver": "^2.0.5",
    "fs-extra": "^10.0.0",
    "node-html-parser": "^3.3.5",
    "pako": "^2.0.3",
    "pica": "7.0.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "react-colorful": "^5.2.2",
    "svgo": "^2.3.0",
    "typescript": "^4.3.2",
    "uvu": "^0.5.1"
  },
  "sideEffects": false,
  "type": "module",
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": false
  }
}
